    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boardzilla/boardzilla-devtools.git"
  },
  "contributors": [
    "Andrew Hull <aghull@gmail.com>",
    "Joshua Hull <josh@fireflop.com>"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/boardzilla/boardzilla-devtools/issues"
  },
  "goBinary": {
